{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "npx nuxi typecheck",
    "lint": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lintfix": "npm run lint -- --fix && npm run typecheck",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "npm run lintfix"
    ]
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@types/node": "^18.16.19",
    "@types/uuid": "^9.0.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "nuxt": "^3.6.3",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "vue-tsc": "^1.8.5"
  }
}
